<h1>Viewing Party</h1>

<% if session[:user_id] %>
  <%= link_to "Log Out", logout_path, method: :delete %>
  <ul class="user-list">
    <% @users.each do |user| %>
      <%= link_to "#{user.name}", "/users/#{user.id}" %>
    <% end %>
  </ul>
<% else %>
  <%= button_to "Create New User", new_user_path %>

  <%= link_to "Log In", login_path %>
<% end  %>