<%= button_to 'Discover Page', user_discover_index_path(@user), method: :get %>

<div class="movies">
  <% @movies.each do |movie| %>
    <div class="movie">
      <div class="title"><%= link_to movie[:title], url: "/users/#{@user}/movies/#{movie[:id]}" %></div>
      <#% require 'pry'; binding.pry %>
      <div class="vote-average"><%= movie[:vote_average] %></div>
    </div>
  <% end %>
</div>